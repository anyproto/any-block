{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/definitions/InvitePayload",
    "definitions": {
        "InvitePayload": {
            "properties": {
                "creatorIdentity": {
                    "type": "string"
                },
                "creatorName": {
                    "type": "string"
                },
                "creatorIconCid": {
                    "type": "string"
                },
                "creatorIconEncryptionKeys": {
                    "items": {
                        "$ref": "#/definitions/anytype.model.FileEncryptionKey"
                    },
                    "type": "array"
                },
                "aclKey": {
                    "type": "string",
                    "format": "binary",
                    "binaryEncoding": "base64"
                },
                "spaceId": {
                    "type": "string"
                },
                "spaceName": {
                    "type": "string"
                },
                "spaceIconCid": {
                    "type": "string"
                },
                "spaceIconOption": {
                    "type": "integer"
                },
                "spaceUxType": {
